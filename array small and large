#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int N;
    scanf("%d",&N);
    int arr[N];
    for (int i=0 ; i<N;i++){
        scanf("%d",&arr[i]);
    }
    int small=arr[0];
    int large=arr[0];
    for (int i=1; i<N;i++){
        if (arr[i]<small)
            small = arr[i];
        if (arr[i]>large)
            large = arr[i];
    }
    printf("%d %d",small,large);

    return 0;
}
